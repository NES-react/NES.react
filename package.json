{
  "name": "nes.react",
  "version": "0.0.1",
  "description": "A REACT Library built on top of the NES.css Framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev:init": "npm install && cd ./example && npm install",

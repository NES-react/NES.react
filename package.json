  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NES-react/nes.react/issues"
  },
  "homepage": "https://github.com/NES-react/nes.react#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "react": "^16.10.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "peerDependencies": {
    "react": "^16.10.2"
  }
